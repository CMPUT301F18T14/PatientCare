<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mukha.picmymedcode" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        <activity android:name=".View.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.ProblemActivity"
            android:label="Problem"/>
        <activity android:name=".View.AddProblemActivity"
            android:label="New Problem"/>
        <activity android:name=".View.SignUpActivity"
            android:label="Sign Up"/>
        <activity android:name=".View.newUsernameActivity"
            android:label="Enter Username"/>
    </application>

</manifest>

